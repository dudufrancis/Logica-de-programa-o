programa
{
	
	funcao inicio()
	{

		inteiro numeros[13] = {10,9,80,72,74,5,62,36,44,7,3,17,66}
		inteiro numeroMaior

		para(inteiro i = 0; i <= 13; i ++){
			para(inteiro j = i; j < 13; j++){

				se( numeros[i] > numeros[j]) {
									
					numeroMaior = numeros[i]
					numeros[i] = numeros[j]
				  	numeros[j] = numeroMaior
				}
			}
		}

		para(inteiro i = 0; i < 13; i++){
			escreva("\nA ordem Ã©: "+ numeros[i])
		}		
	}
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
}
